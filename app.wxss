/**app.wxss**/
.container {
  height: 100%;
  box-sizing: border-box;
  font-size: 32rpx;
  overflow: hidden;
} 
.container::before{
  content: "";
  width: 100%;
  height: 2rpx;
  background: #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.cut{
  width: 100%;
  height: 12rpx;
  background-color: #f2f2f2;
  display: block;
}
view text{
  margin: 0;
  padding: 0;
}
.clearfix::after{
  content: "";
  visibility: hidden;
  overflow: hidden;
  display: block;
  clear: both;
}
.fl{
  float: left;
}